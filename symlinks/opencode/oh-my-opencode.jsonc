// bunx oh-my-opencode doctor --verbose
// https://github.com/code-yeongyu/oh-my-opencode/blob/dev/src/config/schema.ts
{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "disabled_hooks": [
    // "todo-continuation-enforcer",
    "directory-readme-injector",
    "startup-toast"
  ],
  // re-add websearch in opencode.jsonc to fix the timed out issue
  "disabled_mcps": ["websearch"],
  "disabled_skills": ["agent-browser"],
  "browser_automation_engine": {
    // `npm install -g agent-browser` -> `agent-browser install`
    "provider": "agent-browser"
  },
  "git_master": {
    "commit_footer": false,
    "include_co_authored_by": false
  },
  "background_task": {
    "modelConcurrency": {
      "anthropic/claude-opus-4-5-20251101": 3,
      "google/antigravity-claude-opus-4-5-thinking": 3,
      "github-copilot/claude-opus-4.5": 3
    }
  },
  // "tmux": {
  //   "enabled": true
  // },
  // https://github.com/code-yeongyu/oh-my-opencode/blob/64825158a732467fc7310bfc976c6733b227931b/src/shared/model-requirements.ts
  "agents": {
    "sisyphus": {
      // "model": "github-copilot/claude-opus-4.5",
      // "variant": "thinking"
      "model": "synthetic/hf:moonshotai/Kimi-K2.5"
      // "model": "zai-coding-plan/glm-4.7"
    },
    "hephaestus": {
      "model": "github-copilot/gpt-5.2-codex",
      "variant": "medium"
    },
    "atlas": {
      "model": "synthetic/hf:moonshotai/Kimi-K2.5"
      // "model": "github-copilot/claude-sonnet-4.5"
    },
    "prometheus": {
      // "model": "github-copilot/claude-opus-4.5",
      // "variant": "thinking"
      "model": "synthetic/hf:moonshotai/Kimi-K2.5"
      // "model": "github-copilot/gpt-5.2"
    },
    "metis": {
      // "model": "github-copilot/claude-opus-4.5",
      // "variant": "thinking"
      "model": "synthetic/hf:moonshotai/Kimi-K2.5"
      // "model": "github-copilot/gpt-5.2"
    },
    "momus": {
      "model": "github-copilot/gpt-5.2",
      "variant": "medium"
    },
    "sisyphus-junior": {
      "model": "zai-coding-plan/glm-4.7"
    },
    "oracle": {
      "model": "github-copilot/gpt-5.2",
      "variant": "high"
    },
    "librarian": {
      "model": "zai-coding-plan/glm-4.7"
    },
    "explore": {
      // "model": "github-copilot/grok-code-fast-1"
      // "model": "github-copilot/gpt-5-mini"
      "model": "google/antigravity-gemini-3-flash"
    },
    "multimodal-looker": {
      "model": "google/antigravity-gemini-3-flash"
    }
  },
  "categories": {
    "visual-engineering": {
      "model": "google/antigravity-gemini-3-pro"
    },
    "ultrabrain": {
      "model": "github-copilot/gpt-5.2-codex",
      "variant": "xhigh"
    },
    "deep": {
      "model": "github-copilot/gpt-5.2-codex",
      "variant": "medium"
    },
    "artistry": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high"
    },
    "quick": {
      "model": "google/antigravity-gemini-3-flash"
    },
    "unspecified-low": {
      "model": "zai-coding-plan/glm-4.7"
    },
    "unspecified-high": {
