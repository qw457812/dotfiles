// https://github.com/code-yeongyu/oh-my-opencode/blob/dev/src/config/schema.ts
{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "disabled_hooks": [
    // "todo-continuation-enforcer",
    "startup-toast"
  ],
  "disabled_skills": ["playwright"],
  "git_master": {
    "commit_footer": false,
    "include_co_authored_by": false
  },
  "background_task": {
    "modelConcurrency": {
      "anthropic/claude-opus-4-5-20251101": 3,
      "google/antigravity-claude-opus-4-5-thinking": 3,
      "github-copilot/claude-opus-4.5": 3
    }
  },
  "agents": {
    "oracle": {
      "model": "github-copilot/gpt-5.2",
      "variant": "high"
    },
    "librarian": {
      // "model": "opencode/glm-4.7-free"
      "model": "zai-coding-plan/glm-4.7"
    },
    "explore": {
      "model": "google/antigravity-gemini-3-flash"
    },
    "multimodal-looker": {
      "model": "google/antigravity-gemini-3-flash"
    },
    "Momus (Plan Reviewer)": {
      "model": "github-copilot/gpt-5.2",
      "variant": "medium"
    },
    "Sisyphus-Junior": {
      // "model": "anthropic/claude-sonnet-4-5-20250929"
      "model": "zai-coding-plan/glm-4.7"
    }
  },
  // https://github.com/code-yeongyu/oh-my-opencode/blob/8cc995891e0a6a08c8d5fedc6ab42d2faad022bb/src/tools/delegate-task/constants.ts#L158-L166
  // https://github.com/code-yeongyu/oh-my-opencode/blob/ffe82c1dbdeb4496ed8e13631ba74fbf1782a6bb/src/cli/model-fallback.ts#L49-L86
  "categories": {
    "visual-engineering": {
      "model": "google/antigravity-gemini-3-pro"
    },
    "ultrabrain": {
      "model": "github-copilot/gpt-5.2-codex",
      "variant": "high"
    },
    "artistry": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high"
    },
    "quick": {
      // "model": "anthropic/claude-haiku-4-5-20251001"
      "model": "google/antigravity-gemini-3-flash"
    },
    "unspecified-low": {
      // "model": "anthropic/claude-sonnet-4-5-20250929"
      "model": "zai-coding-plan/glm-4.7"
    },
    "unspecified-high": {
      // "model": "github-copilot/claude-opus-4.5",
      // "variant": "high"
      "model": "zai-coding-plan/glm-4.7"
    },
    "writing": {
      "model": "google/antigravity-gemini-3-flash"
    }
  }
}
